<div class="share-ownership">
  <p>Share this lunchbox with another parent/guardian:</p>
  <%= form_tag({controller: "account_ownerships", action: "new"}, method: "get") do %>
    <%= text_field :user, nil, :placeholder => "Enter their email address here" %>
    <%= submit_tag "Share Lunchbox" %>
  <% end %>
</div>
