<%= form_for @vendor do |f| %>

  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div class="field">
    <%= f.label :phone_number %>
    <%= f.telephone_field :phone_number %>
  </div>

  <div class="card-align-left">

    <div class="actions">
      <%= submit_tag "Submit" %>
    </div>
  </div>

<% end %>